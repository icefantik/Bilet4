<Window x:Class="ExamBilet.LargestNumberOutstandApp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamBilet"
        mc:Ignorable="d"
        Title="Виды мебели с большим не выполненых заявок " Height="400" Width="400">
    <Grid>
        <DataGrid x:Name="tableLargestNumber" IsReadOnly="True" AutoGenerateColumns="True" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Background="#FFFFFF" ItemsSource="{DynamicResource ResourceKey=LargestNumOutstand}"/>
    </Grid>
</Window>
