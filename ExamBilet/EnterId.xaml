<Window x:Class="ExamBilet.EnterId"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamBilet"
        mc:Ignorable="d"
        Title="Удаление спецификации" Height="195.254" Width="440">
    <Grid>
        <Grid.ColumnDefinitions>
            <!-- Разбиение на ширину -->
            <ColumnDefinition Width="13*"/>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="52*"/>
            <ColumnDefinition Width="12*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <!-- Разбиение на длины -->
            <RowDefinition Height="40*"/>
            <RowDefinition Height="15*"/>
            <RowDefinition Height="50*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="id" Grid.Row="1" Grid.Column="1"/>
        <Button Grid.Row="1" Grid.Column="2" Click="ClickDeleteOnId">Удалить</Button>
    </Grid>
</Window>
